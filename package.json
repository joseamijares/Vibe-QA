{
  "name": "vibe-qa",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:widget": "vite build --config vite.widget.config.ts",
    "deploy:widget": "npm run build:widget && ./scripts/deploy-widget-supabase.sh",
    "deploy:widget:staging": "npm run build:widget && ./scripts/deploy-widget-supabase.sh staging",
    "deploy:widget:beta": "npm run build:widget && ./scripts/deploy-widget-supabase.sh beta",
    "dev:widget": "vite serve public --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "precommit": "npm run format && npm run typecheck && bash .claude/hooks/run-quality-check.sh",
    "verify-email-tables": "tsx scripts/verify-email-tables.ts",
    "test-email": "tsx scripts/test-email-invitation.ts",
    "test-email-direct": "tsx scripts/test-email-direct.ts",
    "deploy-email": "bash scripts/deploy-email-functions.sh",
    "seed-feedback": "tsx scripts/seed-feedback.ts",
    "test-feedback": "tsx scripts/test-feedback-submission.ts"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@stripe/stripe-js": "^7.7.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.28.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "sonner": "^2.0.6",
    "stripe": "^18.3.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "wouter": "^3.0.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/form-data": "^2.5.2",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "dotenv": "^17.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "form-data": "^4.0.4",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "tsx": "^4.20.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
